<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="600.0" maxWidth="1200.0" prefHeight="600.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdminController">
   <children>
      <VBox layoutX="24.0" layoutY="20.0" spacing="2.0">
         <children>
            <Label fx:id="sellerWelcomeName" text="Admin name">
               <font>
                  <Font name="Lucida Sans Regular" size="14.0" />
               </font>
            </Label>
            <Label fx:id="sellerWelcomeID" text="Admin ID">
               <font>
                  <Font name="Lucida Sans Regular" size="14.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="405.0" layoutY="7.0" prefHeight="44.0" prefWidth="410.0" text="Selling Operations" textAlignment="CENTER">
         <font>
            <Font name="Lucida Sans Unicode" size="24.0" />
         </font>
      </Label>
      <TabPane layoutY="74.0" prefHeight="536.0" prefWidth="1200.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="Factories and Concessionaires">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="carsExposedCustomerTable" layoutX="1.0" prefHeight="504.0" prefWidth="386.0">
                           <columns>
                              <TableColumn prefWidth="90.0" resizable="false" text="FACTORY ID">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="carBrand" />
                                 </cellValueFactory>
                              </TableColumn>
                              <TableColumn prefWidth="102.00003051757812" resizable="false" text="CAR CAPACITY">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="carModel" />
                                 </cellValueFactory>
                              </TableColumn>
                              <TableColumn prefWidth="119.0" resizable="false" style="-fx-alignment: CENTER;" text="CARS PRODUCED">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="carColor" />
                                 </cellValueFactory>
                              </TableColumn>
                              <TableColumn prefWidth="75.0" text="Column X" />
                           </columns>
                           <items>
                              <FXCollections fx:factory="observableArrayList" />
                           </items>
                        </TableView>
                        <TableView layoutX="405.0" prefHeight="504.0" prefWidth="399.0">
                          <columns>
                            <TableColumn prefWidth="140.0" text="CONCESSIONAIRE ID" />
                            <TableColumn minWidth="0.0" prefWidth="106.0" text="CAR CAPACITY" />
                              <TableColumn prefWidth="81.0" text="Nº SELLERS" />
                              <TableColumn prefWidth="75.0" text="Column X" />
                          </columns>
                        </TableView>
                        <Separator layoutX="386.0" orientation="VERTICAL" prefHeight="503.0" prefWidth="23.0" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab text="Sales History">
               <content>
                  <AnchorPane prefHeight="400.0" prefWidth="700.0">
                     <children>
                        <TableView prefHeight="504.0" prefWidth="399.0">
                           <columns>
                              <TableColumn prefWidth="140.0" text="CONCESSIONAIRE ID" />
                              <TableColumn minWidth="0.0" prefWidth="106.0" text="CAR CAPACITY" />
                              <TableColumn prefWidth="81.0" text="Nº SELLERS" />
                              <TableColumn prefWidth="75.0" text="Choice" />
                           </columns>
                        </TableView>
                        <Separator layoutX="399.0" layoutY="1.0" orientation="VERTICAL" prefHeight="503.0" prefWidth="23.0" />
                        <TableView fx:id="pendingBookingsTable" layoutX="422.0" prefHeight="508.0" prefWidth="622.0">
                           <columns>
                              <TableColumn maxWidth="75.0" prefWidth="75.0" resizable="false" style="-fx-alignment: CENTER;" text="BOOKING&#10;    TYPE">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="bookingType" />
                                 </cellValueFactory>
                              </TableColumn>
                              <TableColumn prefWidth="555.0" text="CAR DETAILS">
                                 <columns>
                                    <TableColumn maxWidth="155.0" prefWidth="155.0" resizable="false" style="-fx-alignment: CENTER_RIGHT;" text="BRAND / MODEL">
                                       <cellValueFactory>
                                          <PropertyValueFactory property="brandModel" />
                                       </cellValueFactory>
                                    </TableColumn>
                                    <TableColumn prefWidth="80.0" resizable="false" style="-fx-alignment: CENTER;" text="COLOR">
                                       <cellValueFactory>
                                          <PropertyValueFactory property="carColor" />
                                       </cellValueFactory>
                                    </TableColumn>
                                    <TableColumn prefWidth="50.0" resizable="false" style="-fx-alignment: CENTER;" text="YEAR">
                                       <cellValueFactory>
                                          <PropertyValueFactory property="year" />
                                       </cellValueFactory>
                                    </TableColumn>
                                    <TableColumn maxWidth="90.0" prefWidth="90.0" resizable="false" style="-fx-alignment: CENTER_RIGHT;" text="KILOMETERS">
                                       <cellValueFactory>
                                          <PropertyValueFactory property="kilometers" />
                                       </cellValueFactory>
                                    </TableColumn>
                                 </columns>
                              </TableColumn>
                              <TableColumn maxWidth="100.0" prefWidth="90.0" resizable="false" style="-fx-alignment: CENTER;" text="PAYMENT&#10;    TYPE">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="paymentType" />
                                 </cellValueFactory>
                              </TableColumn>
                              <TableColumn maxWidth="98.0" resizable="false" style="-fx-alignment: CENTER_RIGHT;" text="AMOUNT">
                                 <cellValueFactory>
                                    <PropertyValueFactory property="amount" />
                                 </cellValueFactory>
                              </TableColumn>
                           </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
